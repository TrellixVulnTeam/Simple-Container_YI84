f = open("/dev/urandom", "r")
data = ""
for i in range(10):
    data += f.read(10000000) # 10mb
    i += 1
    print("%dmb" % (i*10,))
for i in range(10):
    data += f.read(10000000) # 10mb
    print("%d mb" % (i*10,))
for i in range(10):
    data += f.read(10000000) # 10mb
    print(i)
f = open("/dev/urandom", "rb")
for i in range(10):
    data += f.read(10000000) # 10mb
    print(i)
data = b""
for i in range(10):
    data += f.read(10000000) # 10mb
    print(i)
for i in range(20):
    data += f.read(10000000) # 10mb
    print(i)
exit()
f = open("/dev/urandom", "rb")
data = b""
f.read(100000)
data += f.read(100000000)
exit
exit()
f = open("/dev/urandom", "rb")
data = b""
data += f.read(10000000)
exit()
f = open("/dev/urandom", "rb")
data = b""
data+=f.read(100000)
exit
exit()
f = open("/dev/urandom", "r")
f = open("/dev/urandom", "rb")
f.read(10000)
data = b""
data += f.read(1000000000)
exit()
f = open("/dev/urandom", "rb")
data = b""
data+=f.read(10000000)
exit()
